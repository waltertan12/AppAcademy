<h1>All Cats</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Color</th>
    <th>Sex</th>
    <th>Age</th>
    <th>Description</th>
  </tr>
  <% @cats.each do |cat| %>
    <tr>
      <td><%= link_to cat.name, cat_path(cat.id) %></td>
      <td><%= cat.color %></td>
      <td><%= cat.sex %></td>
      <td><%= cat.age %></td>
      <td><%= cat.description %></td>
    </tr>
  <% end %>
</table>
<br>
<%= link_to "Create a new Cat", new_cat_path %>
